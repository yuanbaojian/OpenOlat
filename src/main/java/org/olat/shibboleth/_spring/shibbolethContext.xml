				<entry key='${shibboleth.user.mapping.email.shib}' value="email" />
				<entry key='${shibboleth.user.mapping.first.name.shib}' value="firstName" />
				<entry key='${shibboleth.user.mapping.last.name.shib}' value="lastName" />

