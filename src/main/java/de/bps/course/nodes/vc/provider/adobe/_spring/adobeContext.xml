<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
  http://www.springframework.org/schema/beans 
  http://www.springframework.org/schema/beans/spring-beans.xsd">
  
  <bean id="adobeDefaultConfig" class="de.bps.course.nodes.vc.provider.adobe.AdobeConnectConfiguration">
  	<!-- general -->
  	<property name="createMeetingImmediately" value="${vc.adobe.createMeetingImmediately}" />
  	<!-- Adobe Connect specific -->
  	<property name="guestAccessAllowed" value="${vc.adobe.guestAccessAllowed}" />
	<property name="guestStartMeetingAllowed" value="${vc.adobe.guestStartMeetingAllowed}" />
	<property name="useMeetingDates" value="${vc.adobe.useMeetingDates}" />
  </bean>
  
</beans>